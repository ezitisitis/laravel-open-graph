{
    "name": "ezitisitis/laravel-open-graph",
    "description": "Setting Open Graph for Laravel.",
    "type": "library",
    "license": "GPL-3.0-only",
    "authors": [
        {
            "name": "Marks Bogdanovs",
            "email": "mail@ezitisitis.com"
        }
    ],
    "keywords": [
        "laravel",
        "opengraph",
        "social",
        "keyword",
        "description"
    ],
    "homepage": "https://github.com/ezitisitis/laravel-open-graph",
    "require": {
        "php": ">=7.1.3",
        "illuminate/support": ">=5.6",
        "illuminate/contracts": ">=5.6"
    },
    "require-dev": {
        "mockery/mockery": "^1.3.1",
        "phpunit/phpunit": ">=7.0"
    },
    "autoload": {
        "psr-4": {
            "Ezitisitis\\LaravelOpenGraph\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ezitisitis\\LaravelOpenGraph\\Test\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ezitisitis\\LaravelOpenGraph\\OpenGraphServiceProvider"
            ],
            "aliases": {
                "OpenGraph": "Ezitisitis\\LaravelOpenGraph\\Facades\\OpenGraph"
            }
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "kylekatarnls/update-helper": true
        }
    }
}
